{
  "name": "ylitse-web",
  "version": "0.0.1",
  "description": "Ylitse web UI",
  "main": "index.js",
  "scripts": {
    "eslint:check": "eslint-config-prettier src/App.tsx",
    "eslint:fix": "eslint --fix .",
    "eslint": "eslint .",
    "lint": "tsc && npm run eslint:check && npm run eslint",
    "licenses": "npm install license-checker --save-dev && npx license-checker --json > licenses.json",
    "test:unit": "jest",
    "test:e2e": "npm run wait && cypress run",
    "test": "npm run test:unit && npm run test:e2e",
    "build": "webpack",
    "start": "webpack serve --open",
    "wait": "wait-on http://localhost:8080/version && wait-on http://localhost:8082"
  },
  "author": "",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.2",
    "eslint-plugin-jsonc": "^2.6.0",
    "fp-ts": "^2.13.1",
    "i18next": "^22.4.3",
    "i18next-browser-languagedetector": "^7.0.1",
    "io-ts": "^2.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-i18next": "^12.1.1",
    "react-redux": "^8.0.2",
    "react-router-dom": "^6.3.0",
    "reselect": "^4.1.6",
    "styled-components": "5.3.5",
    "uid": "^2.0.2",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@babel/preset-env": "^7.18.6",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/html-webpack-plugin": "^3.2.9",
    "@types/jest": "^28.1.3",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/styled-components": "^5.1.25",
    "@types/webpack": "^5.28.5",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "babel-jest": "^28.1.1",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "cypress": "^13.4.0",
    "dotenv": "^16.4.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.4",
    "jest": "^28.1.1",
    "jest-environment-jsdom": "^28.1.1",
    "license-checker": "^25.0.1",
    "msw": "^0.47.4",
    "node-2fa": "^2.0.3",
    "prettier": "^3.0.3",
    "react-test-renderer": "^18.2.0",
    "ts-loader": "^8.4.0",
    "typescript": "^4.7.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "overrides": {
    "postcss": "^8.4.31"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "jsdom",
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "./src/test/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1"
    }
  }
}
