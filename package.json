{
  "name": "ylitse-web",
  "version": "2.0.0",
  "description": "Ylitse web UI",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "tsc && tsc -p cypress/tsconfig.json && npm run eslint",
    "licensesjson:create": "license-checker-rseidelsohn --json > licenses.json",
    "test:unit": "jest --verbose",
    "test:e2e": "npm run wait && cypress run",
    "test": "npm audit && npm run test:unit && npm run test:e2e",
    "build": "npm run licensesjson:create && webpack --mode production",
    "start": "npm run licensesjson:create && webpack serve --mode development --open",
    "wait": "wait-on http://localhost:8080/mentors && wait-on http://localhost:8082"
  },
  "author": "SOS Children's Villages Finland",
  "license": "AGPL-3.0-or-later",
