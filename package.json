{
  "name": "ylitse-web",
  "version": "2.0.0",
  "description": "Ylitse web UI",
  "main": "index.js",
  "scripts": {
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "tsc && npm run eslint",
    "licensesjson:create": "license-checker --json > licenses.json",
    "test:unit": "jest",
    "test:e2e": "npm run wait && cypress run",
    "test": "npm audit --audit-level high && npm run test:unit && npm run test:e2e",
    "build": "npm run licensesjson:create && webpack",
    "start": "npm run licensesjson:create && webpack serve --open",
    "wait": "wait-on http://localhost:8080/mentors && wait-on http://localhost:8082"
  },
  "author": "SOS Children's Villages Finland",
  "license": "AGPL-3.0-or-later",
  "dependencies": {
    "@reduxjs/toolkit": "2.9.0",
    "fp-ts": "2.16.11",
    "i18next": "25.5.2",
    "i18next-browser-languagedetector": "8.2.0",
    "io-ts": "2.2.22",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hot-toast": "2.6.0",
    "react-i18next": "15.7.3",
    "react-redux": "9.2.0",
    "react-router-dom": "6.30.1",
    "reselect": "5.1.1",
    "styled-components": "5.3.11",
    "uid": "2.0.2",
    "whatwg-fetch": "3.6.20"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.28.4",
    "@babel/preset-env": "7.28.3",
    "@babel/preset-react": "7.27.1",
    "@babel/preset-typescript": "7.27.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/html-webpack-plugin": "3.2.9",
    "@types/jest": "28.1.3",
    "@types/react": "18.3.23",
    "@types/react-dom": "18.3.7",
    "@types/styled-components": "5.1.25",
    "@types/webpack": "5.28.5",
    "@typescript-eslint/eslint-plugin": "8.43.0",
    "@typescript-eslint/parser": "8.43.0",
    "babel-jest": "28.1.1",
    "copy-webpack-plugin": "11.0.0",
    "css-loader": "7.1.2",
    "cypress": "14.5.4",
    "dotenv": "17.2.2",
    "eslint": "8.57.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-better-styled-components": "1.1.2",
    "eslint-plugin-jest": "28.14.0",
    "eslint-plugin-jsonc": "2.6.0",
    "eslint-plugin-prettier": "5.5.4",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "4.6.0",
    "file-loader": "6.2.0",
    "html-webpack-plugin": "5.6.4",
    "jest": "28.1.1",
    "jest-fixed-jsdom": "0.0.10",
    "license-checker": "25.0.1",
    "msw": "2.11.2",
    "node-2fa": "2.0.3",
    "prettier": "3.6.2",
    "react-test-renderer": "18.2.0",
    "ts-loader": "9.5.4",
    "typescript": "5.5.4",
    "wait-on": "8.0.4",
    "webpack": "5.101.3",
    "webpack-cli": "6.0.1",
    "webpack-dev-server": "4.15.2"
  },
  "overrides": {
    "postcss": "8.4.31"
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "testEnvironment": "jest-fixed-jsdom",
    "testEnvironmentOptions": {
      "customExportConditions": [
        ""
      ]
    },
    "transform": {
      "\\.[jt]sx?$": "babel-jest",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js"
    },
    "setupFilesAfterEnv": [
      "@testing-library/jest-dom",
      "./src/test/setup.ts"
    ],
    "moduleNameMapper": {
      "^@/(.*)": "<rootDir>/src/$1"
    }
  }
}
